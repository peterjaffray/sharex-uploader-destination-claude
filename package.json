{
  "name": "sharex-cloudfront-uploader",
  "version": "1.0.0",
  "description": "Get around blockers and share your images with AI more effectively. Host your screenshots on CloudFront for AI tools like Claude Code.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "mocha tests/unit.test.js --timeout 10000",
    "test:integration": "mocha tests/integration.test.js --timeout 10000", 
    "test:e2e": "node test.js",
    "test:watch": "mocha tests/**/*.test.js --watch",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "coverage": "nyc mocha tests/**/*.test.js"
  },
  "keywords": [
    "sharex",
    "cloudfront",
    "s3",
    "uploader",
    "ai",
    "claude-code",
    "image-hosting",
    "screenshot",
    "bypass-blockers",
    "forum-images"
  ],
  "author": "Choice Marketing",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "multer": "^2.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "mocha": "^10.2.0",
    "chai": "^4.3.10",
    "supertest": "^6.3.3",
    "aws-sdk-mock": "^5.8.0",
    "eslint": "^8.56.0",
    "nyc": "^15.1.0"
  }
}
